{% extends 'base.html.twig' %}

{% block title %}SocialNetwork{% endblock %}

{% block body %}
    <h1>SocialNetwork</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ social_network.id }}</td>
            </tr>
            <tr>
                <th>PostByWeek</th>
                <td>{{ social_network.postByWeek }}</td>
            </tr>
            <tr>
                <th>WhichSocialNetwork</th>
                <td>{{ social_network.whichSocialNetwork ? social_network.whichSocialNetwork|join(', ') : '' }}</td>
            </tr>
            <tr>
                <th>Provider</th>
                <td>{{ social_network.provider }}</td>
            </tr>
            <tr>
                <th>Offer</th>
                <td>{{ social_network.offer }}</td>
            </tr>
            <tr>
                <th>Cost</th>
                <td>{{ social_network.cost }}</td>
            </tr>
            <tr>
                <th>InvoicedPrice</th>
                <td>{{ social_network.invoicedPrice }}</td>
            </tr>
            <tr>
                <th>RenewalType</th>
                <td>{{ social_network.renewalType }}</td>
            </tr>
            <tr>
                <th>RenewalDate</th>
                <td>{{ social_network.renewalDate ? social_network.renewalDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>StartDate</th>
                <td>{{ social_network.startDate ? social_network.startDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Commentary</th>
                <td>{{ social_network.commentary }}</td>
            </tr>
            <tr>
                <th>Enable</th>
                <td>{{ social_network.enable ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>SiteClientToServicesBinderId</th>
                <td>{{ social_network.siteClientToServicesBinderId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('social_network_index') }}">back to list</a>

    <a href="{{ path('social_network_edit', {'id': social_network.id}) }}">edit</a>

    {{ include('social_network/_delete_form.html.twig') }}
{% endblock %}
